<TASK name="Тест (скриптовый) внутренних команд процессора" usid="2F335900-3874-422e-8392-5B5CD775AA94" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="x_s_v.xsd">
	<VAR type="STRING" var="list"/>
	<VAR type="STRING" var="empty" value="SUPER"/>
	<CON_WRITE write="empty"/>
	<VAR type="STRING" var="result_task"/>
	<VAR type="BOOL" var="result" value="TRUE"/>
	<CON_WRITE write="result"/>
	<VAR type="BOOL" var="var_result" value="TRUE"/>
	<CON_WRITE write="var_result"/>
	<!--***************************************************************************************************************************************************-->
	<!--***Исходим из предположения, что <IF var="bool"/>  рабочий *************************************************************************-->
	<!--** <OPERATION op1="result" code="INIT" value="FALSE"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = INIT) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = INIT) - сбой"/> - рабочий ************************************************************-->
	<!--***************************************************************************************************************************************************-->
	<OPEN_DEVICE handleName="Console" sign="0x00000021" ok="okc" obligative="TRUE"/>
	<OPEN_DEVICE handleName="Recorder" sign="0x00000071" ok="okr" obligative="TRUE"/>
	<CON_WRITE write="Начало тестирования ..."/>
	<STEP name="OPERATION(INIT, BOOL), IF var=...">
		<VAR type="BOOL" var="true_" value="TRUE"/>
		<VAR type="BOOL" var="false_" value="FALSE"/>
		<VAR type="BOOL" var="var"/>
		<VAR type="BOOL" var="var0"/>
		<VAR type="BOOL" var="var1"/>
		<!--***********     Инициализация булевской переменной        ***************************************************************************-->
		<CON_WRITE write="Инициализация булевской переменной"/>
		<CON_WRITE write="***Простой IF***"/>
		<IF var="false_">
			<CON_WRITE write="false_"/>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix='Тег IF var="bool" - сбой'/>
		</IF>
		<ELSE>
			<CON_WRITE write="FALSE(VAR) +"/>
		</ELSE>
		<IF var="true_">
			<CON_WRITE write="TRUE( VAR) +"/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix='Тег IF var="bool" - сбой'/>
			<CON_WRITE write='IF var="bool" - сбой) - сбой'/>
		</ELSE>
		<CON_WRITE write="присвоение переменной FALSE через другую переменную"/>
		<OPERATION op1="var" code="INIT" op2="false_"/>
		<IF var="var">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = INIT-bool) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = INIT-bool) - сбой"/>
		</IF>
		<ELSE>
			<CON_WRITE write="FALSE(OPERATION(INIT) bool) +"/>
		</ELSE>
		<CON_WRITE write="присвоение переменной TRUE через другую переменную"/>
		<OPERATION op1="var" code="INIT" op2="true_"/>
		<IF var="var">
			<CON_WRITE write="TRUE(OPERATION(INIT) bool) +"/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = INIT-bool) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = INIT-bool) - сбой, result="/>
			<CON_WRITE write="result"/>
		</ELSE>
		<CON_WRITE write="присвоение переменной TRUE напрямую"/>
		<OPERATION op1="var" code="INIT" op2="1" type2="BOOL"/>
		<CON_WRITE write="var"/>
		<IF var="var">
			<CON_WRITE write="TRUE(OPERATION(INIT) int) +"/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = INIT-bool) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = INIT-bool) - сбой, result="/>
			<CON_WRITE write="result"/>
		</ELSE>
		<CON_WRITE write="присвоение переменной FALSE напрямую"/>
		<OPERATION op1="var" code="INIT" op2="FALSE" type2="BOOL"/>
		<IF var="var">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = INIT-bool) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = INIT-bool) - сбой"/>
		</IF>
		<ELSE>
			<CON_WRITE write="FALSE(OPERATION(INIT) const) +"/>
		</ELSE>
		<IF var="result">
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = INIT-bool)  - норма"/>
			<CON_WRITE write="Тег OPERATION(code = INIT-bool)  - норма, result="/>
			<CON_WRITE write="result"/>
		</IF>
		<!--***********     Операции с булевскими переменными         ***************************************************************************-->
		<CON_WRITE write="***Операции с булевскими переменными***"/>
		<!--*************-->
		<!--*** NOT ***-->
		<!--*************-->
		<CON_WRITE write="NOT"/>
		<!--*** 0 ***-->
		<OPERATION op1="var" code="INIT" op2="FALSE" type2="BOOL"/>
		<OPERATION op1="var" code="NOT"/>
		<IF var="var">
			<CON_WRITE write="FALSE(OPERATION(code = NOT)) +"/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = NOT) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = NOT) - сбой"/>
		</ELSE>
		<!--*** 1 ***-->
		<OPERATION op1="var" code="INIT" op2="TRUE" type2="BOOL"/>
		<OPERATION op1="var" code="NOT"/>
		<IF var="var">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = NOT) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = NOT) - сбой"/>
		</IF>
		<IF var="result">
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = NOT)  - норма"/>
			<CON_WRITE write="Тег OPERATION(code = NOT)  - норма"/>
		</IF>
		<!--*************-->
		<!--*** AND ***-->
		<!--*************-->
		<!--*** 00 ***-->
		<CON_WRITE write="AND"/>
		<OPERATION op1="var" code="INIT" op2="FALSE" type2="BOOL"/>
		<OPERATION op1="var" code="AND" op2="var"/>
		<CON_WRITE write="00"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var"/>
		<IF var="var">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = AND) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = AND)(00) - сбой"/>
		</IF>
		<!--*** 01 ***-->
		<OPERATION op1="var" code="INIT" op2="FALSE" type2="BOOL"/>
		<OPERATION op1="var1" code="INIT" op2="TRUE" type2="BOOL"/>
		<OPERATION op1="var" code="AND" op2="var1"/>
		<CON_WRITE write="01"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var1"/>
		<IF var="var">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = AND) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = AND)(01) - сбой"/>
		</IF>
		<!--*** 10 ***-->
		<OPERATION op1="var" code="INIT" op2="TRUE" type2="BOOL"/>
		<OPERATION op1="var0" code="INIT" op2="FALSE" type2="BOOL"/>
		<OPERATION op1="var" code="AND" op2="var0"/>
		<CON_WRITE write="10"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var0"/>
		<IF var="var">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = AND) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = AND)(10) - сбой"/>
		</IF>
		<!--*** 11 ***-->
		<OPERATION op1="var" code="INIT" op2="TRUE" type2="BOOL"/>
		<OPERATION op1="var" code="AND" op2="var"/>
		<CON_WRITE write="11"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var"/>
		<IF var="var">
			<CON_WRITE write="Тег OPERATION(code = AND)(11) - да"/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = AND) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = AND)(11) - сбой"/>
		</ELSE>
		<IF var="result">
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = AND)  - норма"/>
			<CON_WRITE write="Тег OPERATION(code = AND)  - норма"/>
		</IF>
		<!--*************-->
		<!--*** OR ****-->
		<!--*************-->
		<CON_WRITE write="OR"/>
		<!--*** 00 ***-->
		<OPERATION op1="var" code="INIT" op2="FALSE" type2="BOOL"/>
		<OPERATION op1="var" code="OR" op2="var"/>
		<CON_WRITE write="00"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var"/>
		<IF var="var">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = OR) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = OR)(00) - сбой"/>
		</IF>
		<!--*** 01 ***-->
		<OPERATION op1="var" code="INIT" op2="FALSE" type2="BOOL"/>
		<OPERATION op1="var1" code="INIT" op2="TRUE" type2="BOOL"/>
		<OPERATION op1="var" code="OR" op2="var1"/>
		<CON_WRITE write="01"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var1"/>
		<IF var="var">
			<CON_WRITE write="Тег OPERATION(code = OR)(01) +"/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = OR) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = OR)(01) - сбой"/>
		</ELSE>
		<!--*** 10 ***-->
		<OPERATION op1="var" code="INIT" op2="TRUE" type2="BOOL"/>
		<OPERATION op1="var0" code="INIT" op2="FALSE" type2="BOOL"/>
		<OPERATION op1="var" code="OR" op2="var0"/>
		<CON_WRITE write="10"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var0"/>
		<IF var="var">
			<CON_WRITE write="Тег OPERATION(code = OR)(10) - +"/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = OR) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = OR)(10) - сбой"/>
		</ELSE>
		<!--*** 11 ***-->
		<OPERATION op1="var" code="INIT" op2="TRUE" type2="BOOL"/>
		<OPERATION op1="var" code="OR" op2="var"/>
		<CON_WRITE write="11"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var"/>
		<IF var="var">
			<CON_WRITE write="Тег OPERATION(code = OR)(11) - +"/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = OR) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = OR)(11) - сбой"/>
		</ELSE>
		<IF var="result">
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = OR)  - норма"/>
			<CON_WRITE write="Тег OPERATION(code = OR)  - норма"/>
		</IF>
		<!--*************-->
		<!--*** XOR ***-->
		<!--*************-->
		<CON_WRITE write="XOR"/>
		<!--*** 00 ***-->
		<OPERATION op1="var" code="INIT" op2="FALSE" type2="BOOL"/>
		<OPERATION op1="var" code="XOR" op2="var"/>
		<CON_WRITE write="00"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var"/>
		<IF var="var">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = XOR) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = XOR)(00) - сбой"/>
		</IF>
		<!--*** 01 ***-->
		<OPERATION op1="var" code="INIT" op2="FALSE" type2="BOOL"/>
		<OPERATION op1="var1" code="INIT" op2="TRUE" type2="BOOL"/>
		<OPERATION op1="var" code="XOR" op2="var1"/>
		<CON_WRITE write="01"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var1"/>
		<IF var="var">
			<CON_WRITE write="Тег OPERATION(code = XOR)(01) - +"/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = XOR) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = XOR)(01) - сбой"/>
		</ELSE>
		<!--*** 10 ***-->
		<OPERATION op1="var" code="INIT" op2="TRUE" type2="BOOL"/>
		<OPERATION op1="var0" code="INIT" op2="FALSE" type2="BOOL"/>
		<OPERATION op1="var" code="XOR" op2="var0"/>
		<CON_WRITE write="10"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var0"/>
		<IF var="var">
			<CON_WRITE write="Тег OPERATION(code = XOR)(10) - +"/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = XOR) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = XOR)(10) - сбой"/>
		</ELSE>
		<!--*** 11 ***-->
		<OPERATION op1="var" code="INIT" op2="TRUE" type2="BOOL"/>
		<OPERATION op1="var" code="XOR" op2="var"/>
		<CON_WRITE write="11"/>
		<CON_WRITE write="var"/>
		<CON_WRITE write="var"/>
		<IF var="var">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = XOR) - сбой"/>
			<CON_WRITE write="Тег OPERATION(code = XOR)(11) - сбой"/>
		</IF>
		<IF var="result">
			<FORMAT_CONST new="list" var="empty" prefix="Тег OPERATION(code = XOR)  - норма"/>
			<CON_WRITE write="Тег OPERATION(code = XOR)  - норма"/>
		</IF>
	</STEP>
	<!--******************************************************-->
	<!--******************************************************-->
	<STEP name="IF">
		<!--***********    Тег IF         ***************************************************************************-->
		<VAR type="INT" var="m1" value="10"/>
		<VAR type="INT" var="m2" value="10"/>
		<CON_WRITE write="m1"/>
		<CON_WRITE write="m2"/>
		<!--<VAR type="BOOL" var="false" value="FALSE"/>
		-->
		<IF var="m1" code="NEQ" value="10">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF(code = NEQ) - сбой"/>
			<CON_WRITE write="Тег IF(code = NEQ) - сбой"/>
		</IF>
		<ELSE>
			<CON_WRITE write="Тег IF(code = NEQ) - OK cо вторым операндом числом"/>
		</ELSE>
		<CON_WRITE write="m1"/>
		<CON_WRITE write="m2"/>
		<IF var="m1" code="LS" value="m2">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<CON_WRITE write="result"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF(code = LS) - сбой"/>
			<CON_WRITE write="Тег IF(code = LS) - сбой"/>
		</IF>
		<IF var="m1" code="GR" value="m2">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF(code = GR) - сбой"/>
			<CON_WRITE write="Тег IF(code = GR) - сбой"/>
		</IF>
		<IF var="false_">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF - сбой"/>
			<CON_WRITE write="Тег IF - сбой"/>
		</IF>
		<!--***-->
		<IF var="m1" code="EQU" value="m2">
			<EMPTY/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF(code = EQU) - сбой"/>
			<CON_WRITE write="Тег IF(code = EQU) - сбой"/>
		</ELSE>
		<IF var="m1" code="LSEQ" value="m2">
			<EMPTY/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF(code =LSEQ) - сбой"/>
			<CON_WRITE write="Тег IF(code =LSEQ) - сбой"/>
		</ELSE>
		<IF var="m1" code="GREQ" value="m2">
			<EMPTY/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF(code = GREQ) - сбой"/>
			<CON_WRITE write="Тег IF(code = GREQ) - сбой"/>
		</ELSE>
		<IF var="true_">
			<EMPTY/>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF - сбой"/>
			<CON_WRITE write="Тег IF - сбой"/>
		</ELSE>
		<!--******************************************************-->
		<!--******Вложенный IF******-->
		<!--******************************************************-->
		<IF var="true_">
			<IF var="true_">
				<CON_WRITE write="Тег IF(вложенный 11) - ОК"/>
			</IF>
			<ELSE>
				<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
				<FORMAT_CONST new="list" var="empty" prefix="Тег IF - сбой(вложенный 11)"/>
				<CON_WRITE write="Тег IF - сбой"/>
			</ELSE>
		</IF>
		<ELSE>
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF - сбой(вложенный 11)"/>
			<CON_WRITE write="Тег IF - сбой"/>
		</ELSE>
		<IF var="false_">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF - сбой(вложенный 01)"/>
			<CON_WRITE write="Тег IF - сбой"/>
		</IF>
		<ELSE>
			<IF var="true_">
				<CON_WRITE write="Тег IF(вложенный 01) - ОК"/>
			</IF>
			<ELSE>
				<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
				<FORMAT_CONST new="list" var="empty" prefix="Тег IF - сбой(вложенный 01)"/>
				<CON_WRITE write="Тег IF - сбой"/>
			</ELSE>
		</ELSE>
		<IF var="false_">
			<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF - сбой(вложенный 01)"/>
			<CON_WRITE write="Тег IF - сбой"/>
		</IF>
		<IF var="true_">
			<CON_WRITE write="Тег IF(вложенный 10) - ОК"/>
		</IF>
		<ELSE>
			<IF var="false_">
				<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
				<FORMAT_CONST new="list" var="empty" prefix="Тег IF - сбой(вложенный 10)"/>
				<CON_WRITE write="Тег IF - сбой"/>
			</IF>
			<ELSE>
				<CON_WRITE write="Тег IF(вложенный 10) - ОК"/>
			</ELSE>
		</ELSE>
		<ELSE>
			<IF var="false_">
				<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
				<FORMAT_CONST new="list" var="empty" prefix="Тег IF - сбой(вложенный 00)"/>
				<CON_WRITE write="Тег IF - сбой"/>
			</IF>
			<ELSE>
				<CON_WRITE write="Тег IF(вложенный 10) - ОК"/>
			</ELSE>
		</ELSE>
		<IF var="m1" code="EQU" value="m2">
			<CON_WRITE write="Тег IF(вложенный 10) - ОК"/>
		</IF>
		<ELSE>
			<IF var="m1" code="NEQ" value="m2">
				<OPERATION op1="result" code="INIT" op2="FALSE" type2="BOOL"/>
				<FORMAT_CONST new="list" var="empty" prefix="Тег IF - сбой(вложенный 10)"/>
				<CON_WRITE write="Тег IF - сбой"/>
			</IF>
			<ELSE>
				<CON_WRITE write="Тег IF(вложенный 10) - ОК"/>
			</ELSE>
		</ELSE>
		<IF var="result">
			<FORMAT_CONST new="list" var="empty" prefix="Тег IF - норма"/>
			<CON_WRITE write="Тег IF - норма"/>
		</IF>
	</STEP>
	<!--******************************************************-->
	<STEP name="COMPARE">
		<VAR type="INT" var="int10" value="10"/>
		<VAR type="INT" var="int365" value="365"/>
		<VAR type="FLOAT" var="float365E-2" value="365E-2"/>
		<VAR type="FLOAT" var="float3_14E2" value="3.14E2"/>
		<VAR type="FLOAT" var="float3_65" value="3.65"/>
		<VAR type="FLOAT" var="float314" value="314"/>
		<VAR type="BOOL" var="true_equ" value="FALSE"/>
		<VAR type="BOOL" var="true_neq" value="FALSE"/>
		<VAR type="BOOL" var="true_gr" value="FALSE"/>
		<VAR type="BOOL" var="true_ls" value="FALSE"/>
		<VAR type="BOOL" var="true_greq" value="FALSE"/>
